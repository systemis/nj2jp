{
  "presets": [
    "es2016",
    "es2015",
    "react",
    "stage-2"
  ],
  "plugins": [
    "tranform-runtime",
  ],
  "env": {
    "development": {
      "presets": ["react-hmre"],
      "plugins": [
        "transform-react-jsx-source",
        "transform-react-display-name",
        "transform-export-extensions",
        "tranform-runtime",
        "syntax-dynamic-import",
      ],
    },
    "production": {
      "presets": [
        ["es2015", { "modules": false }],
        "stage-2"
      ],
    }
  }
}
